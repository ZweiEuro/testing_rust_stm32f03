{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "cargo_build_debug",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs-debugger",
            // "server": "127.0.0.1:50001", // For connecting to an existing probe-rs-debugger process
            "chip": "STM32F030F4Px",
            "cwd": "${workspaceFolder}",
            "coreConfigs": [
                {
                    "programBinary": "target/thumbv6m-none-eabi/debug/hello",
                    "coreIndex": 0,
                    "svdFile": "stm32f030.svd.patched",
                    "rttEnabled": true,
                    "rttChannelFormats": [
                        {
                            "dataFormat": "String",
                            "channelNumber": 0,
                            "showTimestamps": true
                        }
                    ],
                },
            ],
            "runtimeArgs": [
                "dap-server"
            ],
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false,
                "formatOptions": {
                    "format": "elf" //!MODIFY (or remove). Valid values are: 'bin', 'hex', 'elf'(default), 'idf'
                }
            },
            "connectUnderReset": false,
            "consoleLogLevel": "Console" //Info, Debug
            //"speed": 24000, //!MODIFY (or remove)
            //"probe": "VID:PID:<Serial>", //!MODIFY (or remove)
        },
        {
            "preLaunchTask": "cargo_build_release",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs-release",
            // "server": "127.0.0.1:50001", // For connecting to an existing probe-rs-debugger process
            "chip": "STM32F030F4Px",
            "cwd": "${workspaceFolder}",
            "coreConfigs": [
                {
                    "programBinary": "target/thumbv6m-none-eabi/release/blink",
                    "coreIndex": 0,
                    "svdFile": "stm32f030.svd.patched",
                    "rttEnabled": true,
                    "rttChannelFormats": [
                        {
                            "dataFormat": "String",
                            "channelNumber": 0,
                            "showTimestamps": true
                        }
                    ],
                },
            ],
            "runtimeArgs": [
                "dap-server"
            ],
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false,
                "formatOptions": {
                    "format": "elf" //!MODIFY (or remove). Valid values are: 'bin', 'hex', 'elf'(default), 'idf'
                }
            },
            "connectUnderReset": false,
            "consoleLogLevel": "Console" //Info, Debug
            //"speed": 24000, //!MODIFY (or remove)
            //"probe": "VID:PID:<Serial>", //!MODIFY (or remove)
        },
    ]
}